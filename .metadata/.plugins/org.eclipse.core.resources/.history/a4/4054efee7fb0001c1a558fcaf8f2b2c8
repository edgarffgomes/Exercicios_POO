
public class Paciente {
	private String nome;
	private String cpf;
	private String data;
	
	public void setNome(String n) {
		this.nome = n;
	}
	public String getNome() {
		return this.nome;
	}
	public void setCPF(String c) {
		this.cpf = c;
	}
	public String getCPF() {
		return this.cpf;
	}
	public void setData(String d) {
		this.data = d;
	}
	public String getData() {
		return this.data;
	}
}
